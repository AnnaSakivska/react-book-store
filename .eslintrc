{
  "extends": "airbnb",
  "globals": {
    "SwaggerEditor": false
  },
  "env": {
    "browser": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "arrow-body-style": 0,
    "quotes": [2, "single"],
    "linebreak-style": 0,
    "comma-dangle": ["error", "never"]
  }
}